

div(class="panel panel-left")
  h4(class="panel-title") Pickup Details
  div(class="line-item line-items clearfix address confirmation")
    div(class="left") Order Id
    div(class="right")
      div #{orderInfo.orderId}
  div(class="line-item line-items clearfix address confirmation")
    div(class="left") Order Status
    div(class="right")
      div #{orderInfo.fulfillmentState}
  div(class="line-item line-items clearfix address confirmation")
    div(class="left") Pickup Name
    div(class="right")
      div #{orderInfo.recipientName}  
  div(class="line-item line-items clearfix address confirmation")
    div(class="left") Pickup Time
    div(class="right") 
      div #{orderInfo.pickupTime}
  div(class="clearfix address")
    div(class="left") Pickup Location
    div(class="right")
      div #{locationInfo.storeName}
      div #{locationInfo.addressLine}
      div #{locationInfo.cityStateZip}
